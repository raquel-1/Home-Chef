{
  "name": "home-chef",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint **/*.{js,vue}",
    "scss": "sass --watch assets/styles/scss/:assets/styles/css",
    "format": "prettier --write \"**/*.{js,ts,vue,json,css,md}\"",
    "prepare": "husky i",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "pre-commit": "npm run commitmsg",
    "styles": "stylelint \"**/*.vue\" --fix"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-vue": "^9.25.0",
    "husky": "^9.0.11",
    "postcss-html": "^1.6.0",
    "postcss-sass": "^0.5.0",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "vite": "^5.2.8"
  }
}
